{
  "name": "datafactory",
  "version": "0.0.2",
  "description": "A lightweight node package for creating test / demo data and fixtures.",
  "main": "dist/datafactory.js",
  "scripts": {
    "clean": "rimraf dist",
    "production": "npm run clean && set NODE_ENV=production && webpack --config ./config/webpack/webpack.production.config.js --progress --profile --colors",
    "test": "npm run test:server",
    "test:server": "mocha --opts config/mocha.opts",
    "watch:server": "npm run test:server -- --watch",
    "eslint:source": "eslint ./src/**/*.js",
    "eslint:test": "eslint ./test/**/*.js",
    "karma:firefox": "./node_modules/.bin/karma start --browsers=Firefox",
    "karma:chrome": "./node_modules/.bin/karma start --browsers=Chrome",
    "karma:all": "./node_modules/.bin/karma start --browsers=PhantomJS,Chrome,Firefox",
    "packages": "npm list --depth=0",
    "package:purge": "rm -rf node_modules",
    "package:reinstall": "npm run package:purge && npm install",
    "package:updates": "./node_modules/.bin/ncu",
    "package:upgrade": "npm run package:updates -u"
  },
  "keywords": [
    "factory",
    "fixtures",
    "test data",
    "demo data",
    "eslint",
    "es2015",
    "webpack",
    "es6",
    "universal",
    "hot reloading",
    "isomorphic",
    "babel 6",
    "istanbul"
  ],
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-cli": "^0.1.1",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.1.0",
    "karma-spec-reporter": "0.0.22",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "npm-check-updates": "^2.4.2",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.12.1"
  },
  "babel": {
    "ignore": [
      "packages/babel-cli/src/babel-plugin/templates"
    ]
  },
  "devEngines": {
    "node": ">=4.1 <6",
    "npm": ">=3.1 <5"
  },
  "bugs": "https://github.com/dispatchme/datafactory/issues",
  "homepage": "https://github.com/dispatchme/datafactory",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.1.21",
    "dotenv": "^1.2.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-sourcemap-loader": "^0.3.6",
    "lodash": "^3.10.1",
    "npmlog": "^2.0.0",
    "rimraf": "^2.4.3",
    "webpack-notifier": "1.2.1"
  }
}
